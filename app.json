{
  "expo": {
    "name": "Bodygoal",
    "slug": "bodygoal",
    "version": "1.0.8",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sinansk.bodygoal",
      "versionCode": 9
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": ["expo-localization", "expo-build-properties"],
    "extra": {
      "eas": {
        "projectId": "7f8d6995-dacb-4468-ae43-014943ad7ba0"
      }
    },
    "owner": "sinansk"
  }
}
